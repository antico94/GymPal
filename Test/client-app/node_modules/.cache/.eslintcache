 object[property]\r\n    }\r\n}","D:\\CodeCool\\TW3\\Test\\Test\\client-app\\src\\Pages\\Login&Register&Logout\\Register.jsx",["180"],"﻿import React, {useState} from 'react';\r\nimport \"./login.css\"\r\nimport $ from 'jquery'\r\nimport {variables as API} from \"../../containers/Variables\";\r\n\r\n\r\nconst Register = () => {\r\n\r\n    const [name, setName] = useState(\"\");\r\n    const [email, setEmail] = useState(\"\");\r\n    const [password, setPassword] = useState(\"\");\r\n\r\n    const onSubmitHandler = async (e) => {\r\n        e.preventDefault();\r\n        const response = await fetch(API.REGISTER, {\r\n            method: \"POST\", headers: {\"Content-Type\": \"Application/json\"}, body: JSON.stringify({name, email, password})\r\n        });\r\n        const content = await response.json();\r\n        if (content.Name !== undefined) {\r\n            RegisterSuccess(content.Name)\r\n        } else RegisterFailed();\r\n    }\r\n\r\n    const RegisterSuccess = (_name) => {\r\n        let title = $('#register-title')\r\n        title.text(\"Account created successfully!\")\r\n        $('form').fadeOut(500);\r\n        $('.wrapper').addClass('form-success');\r\n        setTimeout(function () {\r\n            window.location.href = \"http://localhost:3000/login\"\r\n        }, 1500)\r\n    }\r\n\r\n    const RegisterFailed = () => {\r\n        let title = $('#register-title')\r\n        title.text(\"There is already an account with this email.\");\r\n    }\r\n\r\n\r\n    return (<div className=\"login-page\">\r\n        <div className=\"wrapper\">\r\n            <div className=\"container\">\r\n                <h1 id=\"register-title\">Register</h1>\r\n                <form className=\"form\" onSubmit={onSubmitHandler}>\r\n                    <input onChange={e => setName(e.target.value)} type=\"text\" placeholder=\"Name\"/>\r\n                    <input onChange={e => setEmail(e.target.value)} type=\"email\" placeholder=\"Email\"/>\r\n                    <i